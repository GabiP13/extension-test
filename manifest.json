{
    "manifest_version": 3,
    "name": "Image Replacer",
    "description": "Replaces all images on the screen.",
    "version": "1.0",

    "background": {
        "service_worker": "background.js"
    },

    "action": {
      "default_icon": {
        "16": "images/keepitup.jpg"
      }
    },

    "permissions": ["activeTab", "scripting"]
  }